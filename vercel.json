{
    "version": 2,
    "name": "digitalmenu",
    "builds": [
      { "src": "public/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/src/(.*).js", "headers": { "Cache-Control": "public, max-age=31536000, immutable" } },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  