{
    "version": 2,
    "name": "digitalmenu",
    "builds": [
      { "src": "public/**/*", "use": "@vercel/static" },
      { "src": "src/**/*.{js,jsx}", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/src/(.*)", "dest": "/src/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  